pipeline{
  agent any;
  stages{
      stage('build'){
          echo "this is the build stage ..."
          sh 'sleep 5'
      }
      stage('deploy'){
                echo "this is the deploy stage ..."
                sh 'sleep 5'
            }
      stage('test'){
                echo "this is the test stage ..."
                sh 'sleep 5'
            }
  }
}
